{% extends 'base.html.twig' %}

{% block title %}Cliweb{% endblock %}

{% block body %}
    <h1>Cliweb</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cliweb.id }}</td>
            </tr>
            <tr>
                <th>Sku</th>
                <td>{{ cliweb.Sku }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ cliweb.Status }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ cliweb.Price }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ cliweb.Description }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ cliweb.CreatedAt ? cliweb.CreatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ cliweb.Slug }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('cliweb_index') }}">back to list</a>

    <a href="{{ path('cliweb_edit', {'id': cliweb.id}) }}">edit</a>

    {{ include('cliweb/_delete_form.html.twig') }}
{% endblock %}
